{
    "name": "makefile-symbol-provider",
    "displayName": "Makefile Symbol Provider",
    "description": "Makefile Symbol Provider",
    "version": "0.0.1",
    "publisher": "ctf0",
    "repository": "https://github.com/ctf0/makefile-symbol-provider",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other",
        "Programming Languages"
    ],
    "main": "./out/src/extension.js",
    "activationEvents": [
        "onLanguage:makefile",
        "onStartupFinished"
    ],
    "contributes": {
        "languages": [
            {
                "id": "makefile"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "typescript": "^4.6.3",
        "@types/vscode": "^1.52.0",
        "tslint": "^5.20.1",
        "@types/node": "^17.0.23"
    }
}
